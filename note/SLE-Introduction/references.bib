@article{miller2016imaginary,
 ISSN = {0003486X},
 URL = {http://www.jstor.org/stable/44072020},
 abstract = {Suppose that D ⊂ C is a Jordan domain and x, y ϵ ∂D are distinct. Fix k ϵ (4,8), and let η be an SLEk process from x to y in D. We prove that the law of the time-reversal of η is, up to reparametrization, an SLEk process from y to x in D. More generally, we prove that SLEk (ρ₁; ρ₂) processes are reversible if and only if both ρi are at least k/2 — 4, which is the critical threshold at or below which such curves are boundary filling. Our result supplies the missing ingredient needed to show that for all k ϵ (4,8), the so-called conformed loop ensembles CLEk are canonically defined, with almost surely continuous loops. It also provides an interesting way to couple two Gaussian free fields (with different boundary conditions) so that their difference is piecewise constant and the boundaries between the constant regions are SLEk curves.},
 author = {Jason Miller and Scott Sheffield},
 journal = {Annals of Mathematics},
 number = {2},
 pages = {455--486},
 publisher = {Annals of Mathematics},
